@article{olsen_interaction_2022,
	title = {Interaction of socioeconomic status and class relations on reading},
	volume = {54},
	issn = {1086-296X},
	url = {https://doi.org/10.1177/1086296X221116866},
	doi = {10.1177/1086296X221116866},
	abstract = {Student–teacher relationships (STRs) and socioeconomic status (SES) are two widely studied variables that have been found to predict reading achievement in the early grades. The current study extends the literature by investigating the interaction between STRs, measured using the STR Scale completed by teachers, and SES on reading achievement using a nationally representative data set. The study included approximately 8,380 first-grade students and 2,930 teachers, from 860 schools, representing a weighted sample of 3.15 million students. Results from multilevel modeling that controlled for student-, teacher-, and school-level factors found that both STRs and SES were strongly associated with student reading achievement. There was also a statistically significant interaction between close STRs and SES on reading achievement, suggesting that less conflictual STRs were associated with increased reading achievement scores for all students, but were particularly beneficial for students from low SES backgrounds. Educational implications are provided.},
	language = {en},
	number = {3},
	urldate = {2022-09-07},
	journal = {Journal of Literacy Research},
	author = {Olsen, Amanda A. and Huang, Francis L.},
	month = sep,
	year = {2022},
	note = {Publisher: SAGE Publications Inc},
	keywords = {early childhood, hierarchical linear modeling, poverty, quantitative},
	pages = {346--369},
	file = {SAGE PDF Full Text:C\:\\Users\\flh3\\Zotero\\storage\\TMPRBJTK\\Olsen and Huang - 2022 - Interaction of Socioeconomic Status and Class Rela.pdf:application/pdf},
}

